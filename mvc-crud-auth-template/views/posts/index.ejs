
<h2>Posts</h2>
<p><a href="/posts/new">Nuevo</a></p>
<ul>
  <% posts.forEach(p => { %>
    <li>
      <a href="/posts/<%= p.id %>"><%= p.title %></a>
      — <form action="/posts/<%= p.id %>/delete" method="post" style="display:inline;"><button>Eliminar</button></form>
      — <a href="/posts/<%= p.id %>/edit">Editar</a>
    </li>
  <% }) %>
</ul>
